{
    "plugins": [
        "image-captions", "mathjax", "anchorjs"
    ],
    "pluginsConfig": {
        "image-captions": {
            "caption": "Рис._PAGE_IMAGE_NUMBER_. _CAPTION_",
            "align": "left",
            "sharing": {
                "facebook": true,
                "twitter": true,
                "google": true,
                "weibo": true,
                "instapaper": true,
                "vk": true
            }
        },
        "mathjax": {
            "forceSVG": true
        },
        "anchorjs": {
          "placement": "left"
        }
    }
}